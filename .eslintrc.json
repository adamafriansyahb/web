{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "curly": ["error", "multi"],
    "max-params": ["error", 3],
    "no-console": "warn",
    "no-var": "error",
    "no-use-before-define": [
      "error",
      {
        "functions": true,
        "classes": true,
        "variables": true,
        "allowNamedExports": false
      }
    ]
  }
}
